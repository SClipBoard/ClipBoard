{
  "name": "clipboard-sync",
  "version": "1.0.0",
  "description": "跨设备剪切板同步服务",
  "main": "api/server.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch api/server.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "tsc -p api/tsconfig.json",
    "start": "node dist/api/server.js",
    "check": "npm run type-check && npm run lint",
    "type-check": "tsc --noEmit",
    "type-check:backend": "tsc --noEmit -p api/tsconfig.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "db:init": "node -e \"console.log('请手动执行 migrations/001_init_database.sql 文件中的SQL语句来初始化数据库')\"",
    "clean": "rimraf dist node_modules/.vite",
    "packge": "node ./scripts/pack.mjs"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@vercel/node": "^3.0.21",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lucide-react": "^0.312.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "keywords": [
    "clipboard",
    "sync",
    "cross-device",
    "react",
    "express",
    "websocket",
    "mysql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/clipboard-sync.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/clipboard-sync/issues"
  },
  "homepage": "https://github.com/yourusername/clipboard-sync#readme"
}